p0 @ p.0: T0, p1 @ p.1: T1
-------------------------- "Constructor"
[p0, p1] @ p: [T0, T1]

p0 @ &(*p).0: &T0, p1 @ &(*p).1: &T1
------------------------------------ "ConstructorRef"
[p0, p1] @ p: &[T0, T1]

p0 @ &mut (*p).0: &mut T0, p1 @ &mut (*p).1: &mut T1
---------------------------------------------------- "ConstructorRef"
[p0, p1] @ p: &mut [T0, T1]

[p0, p1] @ *p: &T
----------------- "ConstructorMultiRef"
[p0, p1] @ p: &&T

[p0, p1] @ &**p: &T
--------------------- "ConstructorMultiRef"
[p0, p1] @ p: &&mut T

[p0, p1] @ *p: &T
--------------------- "ConstructorMultiRef"
[p0, p1] @ p: &mut &T

[p0, p1] @ &mut **p: &mut T
--------------------------- "ConstructorMultiRef"
[p0, p1] @ p: &mut &mut T

p @ *p: T
---------- "Deref(EatOuter)"
&p @ p: &T

p @ *&p: &mut T
---------------- "Deref(EatOuter)"
&p @ &p: &&mut T

p @ *p: T
------------------ "Deref(EatOuter)"
&mut p @ p: &mut T

&p @ &*p: &T
-------------------------------------- "DerefMutWithShared(EatOuter)"
&p @ p: &mut T, binding_mode(p) = move

&p @ &p: &&mut T
------------------------ "DerefMutWithShared(EatOuter)"
&p @ &mut p: &mut &mut T

x @ &p: &T
------------------------------------ "BindingBorrow"
ref x @ p: T, binding_mode(p) = move

x @ &mut p: &mut T
---------------------------------------- "BindingBorrow"
ref mut x @ p: T, binding_mode(p) = move

let x: T = p
------------ "Binding"
x @ p: T

let mut x: T = p
---------------- "Binding"
mut x @ p: T

p @ p: T
-------------- "ExprSimplification"
